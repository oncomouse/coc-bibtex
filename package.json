{
  "name": "coc-bibtex",
  "version": "1.2.18",
  "description": "Coc list & completion source for BibTeX",
  "main": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "homepage": "https://github.com/oncomouse/coc-bibtex#readme",
  "bugs": {
    "url": "https://github.com/oncomouse/coc-bibtex/issues",
    "email": "oncomouse@gmail.com"
  },
  "author": "Andrew Pilsch <oncomouse@gmail.com> (https://andrew.pilsch.com)",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "prepare": "webpack"
  },
  "keywords": [
    "coc.nvim",
    "neovim",
    "vim",
    "bibtex"
  ],
  "engines": {
    "coc": "^0.0.72"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "bibtex.files": {
          "type": "array",
          "default": [],
          "description": "List of .bib files to read.",
          "items": {
            "type": "string"
          }
        },
        "bibtex.citation": {
          "type": "object",
          "default": {
            "before": "[@",
            "after": "]"
          },
          "properties": {
            "before": {
              "type": "string",
              "description": "What to place before the citation when inserting citations"
            },
            "after": {
              "type": "string",
              "description": "What to place after the citation when inserting citations"
            }
          }
        },
        "bibtex.enable": {
          "type": "boolean",
          "default": true
        },
        "bibtex.silent": {
          "type": "boolean",
          "default": false
        },
        "bibtex.shortcut": {
          "type": "string",
          "default": "BIB"
        },
        "bibtex.triggerCharacters": {
          "type": "array",
          "default": [
            "@"
          ],
          "items": {
            "type": "string"
          }
        },
        "bibtex.triggerPatterns": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "bibtex.filetypes": {
          "type": [
            "array",
            "null"
          ],
          "default": [
            "tex",
            "plaintex",
            "latex",
            "pandoc",
            "markdown"
          ],
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.18",
    "@citation-js/core": "^0.5.0-alpha.9",
    "@citation-js/plugin-bibtex": "^0.5.0-alpha.9",
    "@citation-js/plugin-csl": "^0.5.0-alpha.9",
    "@types/node": "^12.7.2",
    "coc.nvim": "^0.0.73",
    "crypto": "^1.0.1",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  }
}
